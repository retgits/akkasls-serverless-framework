{
  "name": "@retgits/akkasls-serverless-framework",
  "version": "0.1.2",
  "description": "Provider plugin for the Serverless Framework v2.x which adds support for Akka Serverless.",
  "license": "MIT",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "author": "retgits",
  "scripts": {
    "pretest": "npm run lint",
    "test": "./node_modules/.bin/mocha -r ts-node/register 'test/**/*.ts'",
    "watch": "tsc --watch",
    "compile": "rm -rf ./lib && tsc",
    "lint": "eslint src/**/*.ts",
    "build": "npm run compile",
    "release": "standard-version && git push --follow-tags origin main && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/retgits/akkasls-serverless-framework"
  },
  "homepage": "https://github.com/retgits/akkasls-serverless-framework",
  "keywords": [
    "serverless",
    "akka",
    "akkasls"
  ],
  "files": [
    "lib/"
  ],
  "devDependencies": {
    "@serverless/typescript": "^2.15.0",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.14",
    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "chai": "^4.2.0",
    "eslint": "^7.9.0",
    "mocha": "^8.2.1",
    "serverless": "^2.15.0",
    "standard-version": "^9.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@retgits/akkasls-nodewrapper": "^0.1.1"
  }
}
